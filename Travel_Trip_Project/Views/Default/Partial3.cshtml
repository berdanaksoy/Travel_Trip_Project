@using Travel_Trip_Project.Models.Classes
@model List<blog>

<div class="padding">
    <h4>This Week's Top 10</h4>
    <ul class="list list-pad">
        @{ int i = 0;}

        @foreach (var item in Model)
        {
            <li class="list-bg"><span>@{i++;}@i.</span><a href="/Blog/BlogDetails/@item.id"> @item.title</a></li>
        }
    </ul>
    <a href="/Blog/Index/" class="link1">Tüm Blogları Gör</a>
</div>
